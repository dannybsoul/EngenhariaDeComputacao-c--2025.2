#include <iostream>
using namespace std;

int main() {
    int n;

    cout << "digite um numero inteiro: ";
    cin >> n;

    cout << "divisores de " << n << ": ";

    for (int i = 1; i <= n; i++) {
        if (n % i == 0) {
            cout << i << " ";
        }
    }

    cout << endl;

    return 0;
}